{
    "name": "bulckens/messenger",
    "description": "A PHP bridge for OSX's Terminal-Notifier.app",
    "type": "library",
    "license": "Proprietary",
    "authors": [
        {
            "name": "Wout Fierens",
            "email": "wout@mick-wout.com"
        }
    ],
    "repositories": [
        {   "type": "vcs",
            "url": "git@github.com:bulckens/helpers.git",
            "no-api": true
        },
        {   "type": "vcs",
            "url": "git@github.com:bulckens/app-tools.git",
            "no-api": true
        },
        {   "type": "vcs",
            "url": "git@github.com:bulckens/cli-tools.git",
            "no-api": true
        }
    ],
    "require": {
        "bulckens/helpers": "dev-master",
        "bulckens/cli-tools": "dev-master",
        "bulckens/app-tools": "dev-master"
    },
    "dist": {
        "url": "git@github.com:bulckens/messenger.git",
        "type": "git"
    },
    "autoload": {
        "psr-0": {
            "Bulckens": "src/"
        }
    },
    "require-dev": {
        "phpspec/phpspec": "^3.2"
    }
}
